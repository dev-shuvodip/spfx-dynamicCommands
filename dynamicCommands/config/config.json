{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "dynamic-commandset": {
      "components": [
        {
          "entrypoint": "./lib/extensions/dynamicCommandset/DynamicCommandset.js",
          "manifest": "./src/extensions/dynamicCommandset/DynamicCommandset.manifest.json"
        }
      ]
    },
    "custom-list-form-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/customListFormCustomizer/CustomListFormCustomizer.js",
          "manifest": "./src/extensions/customListFormCustomizer/CustomListFormCustomizer.manifest.json"
        }
      ]
    },
    "milestones-poc-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/milestonesPoc/MilestonesPocWebPart.js",
          "manifest": "./src/webparts/milestonesPoc/MilestonesPocWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DynamicCommandSetStrings": "lib/extensions/dynamicCommandset/loc/{locale}.js",
    "CustomListFormCustomizerStrings": "lib/extensions/customListFormCustomizer/loc/{locale}.js",
    "MilestonesPocWebPartStrings": "lib/webparts/milestonesPoc/loc/{locale}.js"
  }
}
